_sym()
{
    COMPREPLY=()
    local current_word="${COMP_WORDS[COMP_CWORD]}"

    local args1="remove rename list add removeall tag removepath removetag tags 
        changepath open backup restore remake runscript makescript path"
    
    local args2=$(sym printnames)
    local args="${args1} ${args2}"

    local ans=($(compgen -W "${args}" "${current_word}"))

    if [[ -n "${ans}" ]]; then
        COMPREPLY=$ans
    fi
}

complete -F _sym -A file sym